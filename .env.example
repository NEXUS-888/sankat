# ============================================
# FRONTEND ENVIRONMENT VARIABLES
# ============================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env files to version control!

# MapTiler API Key
# Get your free API key from: https://www.maptiler.com/cloud/
# Required for map rendering
VITE_MAPTILER_KEY=your_maptiler_api_key_here

# API Base URL (optional, defaults to http://localhost:8000)
# VITE_API_BASE_URL=http://localhost:8000

# Stripe Publishable Key (for payment processing)
# Get from: https://dashboard.stripe.com/test/apikeys
# Use test key (pk_test_...) for development
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# ============================================
# DEVELOPMENT SETTINGS
# ============================================

# Enable debug mode (optional)
# VITE_DEBUG=false

# Environment (development, staging, production)
# VITE_ENV=development
